<?xml version="1.0" encoding="UTF-8"?>
<block xmlns="http://block.namespaces.minibloq.org" xmlns:minibloq="http://minibloq.org" MinibloqVersion="0.8.Beta">
    <properties>
		<name>ServoCenter</name>
		<function>Setter</function>
		<loadAction>load</loadAction>
    </properties>
    <code>
		<dataType>servoMethod</dataType>
		<initialCode>
			<line0>servo( SERVO_CENTER );</line0>
		</initialCode>
	</code>
    <interface>
		<originalSize>
			<width>operationDefaultWidth::</width>
			<height>blockDefaultHeight::</height>
		</originalSize>
        <childFactor>0.9</childFactor>
        <defaultBackgroundColour0>
			<R>68</R>
			<G>162</G>
			<B>53</B>
		</defaultBackgroundColour0>
        <defaultBackgroundColour1>
			<R>52</R>
			<G>123</G>
			<B>42</B>
		</defaultBackgroundColour1>
		<picker>
			<imagesBasePath>thisBlockPath::</imagesBasePath>
			<default>Images/ServoCenterDefault.png</default>
			<hover>Images/ServoCenterHover.png</hover>
			<pressed>Images/ServoCenterPressed.png</pressed>
			<disabled>Images/ServoCenterDisabled.png</disabled>
		</picker>
		<blockButton>
			<has>true</has>
			<imagesBasePath>thisBlockPath::</imagesBasePath>
			<default>Images/ServoCenterBlockDefault.png</default>
			<disabled>Images/ServoCenterBlockDisabled.png</disabled>
		</blockButton>
	</interface>
</block>
